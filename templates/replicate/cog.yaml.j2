# {{ model_name }} - Replicate/Cog configuration
# Generated from config.yaml; system_packages and python_version come from dependencies.

build:
  python_version: "{{ python_version }}"
  system_packages:
{% for pkg in system_packages %}    - {{ pkg }}
{% endfor %}
  python_requirements: requirements.txt
  gpu: true

predict: "predict.py:Predictor"
